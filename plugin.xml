<?xml version='1.0' encoding='utf-8'?>
<plugin id="cordova-plugin-vungle-ad" version="0.0.1" xmlns="http://apache.org/cordova/ns/plugins/1.0" xmlns:android="http://schemas.android.com/apk/res/android">

	<name>vungle</name>
	<js-module name="vungle" src="www/vungle.js">
		<clobbers target="vungle" />
	</js-module>
	<platform name="android">
		<config-file parent="/*" target="res/xml/config.xml">
			<feature name="vungle">
				<param name="android-package" value="cordova.plugin.vungle.ad.vungle" />
			</feature>
		</config-file>
		<config-file parent="/*" target="AndroidManifest.xml">
			
		</config-file>
		<source-file src="src/android/vungle.java" target-dir="src/cordova/plugin-vungle-ad/vungle" />
	</platform>
	</plugin>